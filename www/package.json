{
  "name": "pveme-ui",
  "description": "PVE manager electrified user interface. The npm-published version is for the plugin dev's IDE and contains only the .ts files",
  "type": "module",
  "version": "1.0.5",
  "scripts": {
    "IDE:link_nodejsserver_dir": "npm install --save false --ignore-scripts ../nodejsserver ",
    "check": "tsc --noemit",
    "prepublish": "npm run check",
    "publish:npmlogin": "npm login",
    "publish:publish_to_npm": "npm run prepublish && npm publish"
  },
  "dependencies": {
    "pveme-nodejsserver": "^1.0.0",
    "restfuncs-client": "^2.2.0",
    "react": "^19.1.0",
    "typescript": "=5.1.6"
  }
}
