{
  "type": "module",
  "scripts": {
    "dev_watch": "nodemon -e ts --exec npm run dev_compileAndRunOnce",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "dev_compileAndRunOnce": "clear && npm run build && ts-node killOldProcesses && ts-node index",
    "killOldProcesses": "tsc && ts-node killOldProcesses"
  },
  "dependencies": {
    "execa": "6.1.0",
    "express": "^4.17.13",
    "cookie-parser": "^1.4.7",
    "http-proxy-middleware": "^2.0.6",
    "vite": "^2.7.12"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/express": "^4.17.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "nodemon": "2.0.19",
    "ts-node-dev": "2.0.0"
  }
}
