{
  "name": "pve-nodejsserver",
  "type": "module",
  "scripts": {
    "dev": "npm prune --ignore-scripts && npm run clean && cross-env NODE_ENV=development tsx watch --inspect=9229 server.ts",
    "dev_playground": "tsx watch devPlayground.ts",
    "clean": "tspc --build --clean",
    "build": "tspc --build --force",
    "check": "tspc --build",
    "start": "cross-env NODE_ENV=production node --enable-source-maps server.js"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cookie-parser": "^1.4.7",
    "execa": "6.1.0",
    "express": "^4.17.13",
    "http-proxy-middleware": "^2.0.6",
    "vite": "^2.7.12",
    "restfuncs-server": "^3.2.1",
    "restfuncs-client": "^2.2.0",
    "helmet": "^7.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.11",
    "@types/ws": "^8.18.1",
    "eslint": "^8.22.0",
    "tsx": "^4.7.0",
    "ts-patch": "^3.0.2",
    "restfuncs-transformer": "^1.1.0",
    "cross-env": "^7.0.3",
    "rimraf": "=5.0.5"
  }
}
